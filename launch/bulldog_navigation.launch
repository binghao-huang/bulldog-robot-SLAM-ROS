<?xml version="1.0"?>

<launch>

  <!-- Run the map server -->
  <arg name="map_file" default="/home/bulldog/maps/map1.yaml"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />
  <include file="$(find bulldog_navigation)/launch/amcl.launch" />

  <!--- Run Move Base -->
  <include file="$(find bulldog_navigation)/launch/move_base.launch" />

</launch>
